{
    "build": {
        "content": [{
            "files": [
                "**/*.md",
                "**/*.yml"
            ],
            "exclude": [
                "**/obj/**",
                "**/includes/**",
                "README.md",
                "LICENSE",
                "LICENSE-CODE",
                "ThirdPartyNotices"
            ]
        }],
        "resource": [{
            "files": [
                "**/*.png",
                "**/*.jpg"
            ],
            "exclude": [
                "**/obj/**",
                "**/includes/**"
            ]
        }],
        "overwrite": [],
        "externalReference": [],
        "globalMetadata": {
            "layout": "Conceptual",
            "breadcrumb_path": "/globalization/breadcrumb/toc.json"
        },
        "fileMetadata": {},
        "template": [],
        "dest": "globalization"
    }
}